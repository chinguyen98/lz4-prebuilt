{
  "name": "lz4-prebuilt",
  "version": "1.0.1",
  "description": "LZ4 compression with prebuilt binaries",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "dist/binary/*.node"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "copy-binary": "mkdir -p dist/binary && cp lib/binary/*.node dist/binary/",
    "build": "npm run clean && tsc && npm run copy-binary",
    "prepare": "npm run build"
  },
  "keywords": [
    "lz4",
    "compression",
    "prebuilt",
    "binary"
  ],
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chinguyen98/lz4-prebuilt.git"
  },
  "bugs": {
    "url": "https://github.com/chinguyen98/lz4-prebuilt/issues"
  },
  "homepage": "https://github.com/chinguyen98/lz4-prebuilt#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^20.11.24",
    "typescript": "^5.3.3"
  }
}
